@model CIS174_TestCoreApp.Models.AssignmentViewModel
<h2>Assignment 6.1</h2>

@if (Model.AccessLevel < 2)
{
    <p>You do not have a sufficient access level to view this data.</p>
}
else if (Model.AccessLevel < 8)
{
    <ul>
        @foreach (var student in Model.Students)
        {
            <partial name="_StudentName" model="student" />
        }
    </ul>
}
else
{
    <p>Hello Admin</p>
    <ul>
        @foreach (var student in Model.Students)
        {
            <partial name="_StudentFull" model="student" />
        }
    </ul>
}
